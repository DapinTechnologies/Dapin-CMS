{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.1",
        "africastalking/africastalking": "^3.0",
        "bigbharatjain/laravel-clickatell": "^1.0",
        "doctrine/dbal": "^3.5",
        "guzzlehttp/guzzle": "^7.9",
        "intervention/image": "^2.6.1",
        "kingflamez/laravelrave": "^4.2",
        "laravel/framework": "^9.1",
        "laravel/helpers": "^1.3",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^9.0",
        "namshi/smscountry": "^1.0",
        "obydul/laraskrill": "^1.2",
        "php-flasher/flasher-toastr-laravel": "*",
        "rahulreghunath/textlocal": "^1.0",
        "razorpay/razorpay": "^2.9",
        "spatie/laravel-permission": "^5.5.5",
        "spatie/laravel-translatable": "^6.0.0",
        "spatie/pdf-to-image": "^1.2",
        "srmklive/paypal": "^3.0",
        "stripe/stripe-php": "^15.9",
        "twilio/sdk": "^8.2",
        "unicodeveloper/laravel-paystack": "^1.2",
        "vonage/client": "^4.0",
        "yoeunes/toastr": "^2.3"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        
        "files": [
            "app/Helpers/custom_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
